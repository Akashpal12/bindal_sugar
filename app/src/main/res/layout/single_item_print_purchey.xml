<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="10sp"
    app:cardUseCompatPadding="true"
    app:cardElevation="5dp">

    <LinearLayout
        android:id="@+id/rl_text_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:paddingBottom="0sp">

        <TextView
            android:id="@+id/season"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="@color/black"
            android:text="@string/season1"
            android:textSize="15dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:layout_below="@+id/season"
            >

            <TextView
                android:id="@+id/tvsrno"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="S.No."
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_srno"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvsrno"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            >

            <TextView
                android:id="@+id/tvsurvey_village"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="Survey Village"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_survey_village"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvsurvey_village"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            >

            <TextView
                android:id="@+id/tvsurvey_village_name"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="Survey Village Name"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_survey_village_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvsurvey_village_name"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="@color/black"
            android:text="============================================"
            android:textSize="15dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            >

            <TextView
                android:id="@+id/tvplot_serial_number"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="Plot Sr No"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_plot_serial_number"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvplot_serial_number"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            >

            <TextView
                android:id="@+id/tvgrower_village"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="Grower Village"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_grower_village"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvgrower_village"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            >

            <TextView
                android:id="@+id/tvunique_code"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="Grower Code"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_unique_code"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvunique_code"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/line5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:layout_below="@+id/line4"
            >

            <TextView
                android:id="@+id/tvgrower_name"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="Grower Name"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_grower_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvgrower_name"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/line6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:layout_below="@+id/line5"
            >

            <TextView
                android:id="@+id/tvgrower_father"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="Grower Father"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_grower_father"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvgrower_father"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/line8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:layout_below="@+id/line7"
            >

            <TextView
                android:id="@+id/tveast"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="East"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_east"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tveast"
                android:text=""
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tvwest"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tv_east"
                android:text="West"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_west"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvwest"
                android:text=""
                android:textSize="15dp" />


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/line9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:layout_below="@+id/line8"
            >

            <TextView
                android:id="@+id/tvnorth"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="North"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_north"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvnorth"
                android:text=""
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tvsouth"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tv_north"
                android:text="South"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_south"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvsouth"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>

        <LinearLayout
            android:id="@+id/line10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:layout_below="@+id/line9"
            android:orientation="horizontal"
            >

            <TextView
                android:id="@+id/tvplot_share"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.3"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="Share Per (%)"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_plot_share"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.3"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvplot_share"
                android:text=""
                android:textSize="15dp" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.3"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tv_plot_share"
                android:text=": 100%"
                android:textSize="15dp" />

        </LinearLayout>

        <RelativeLayout
            android:id="@+id/line11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:layout_below="@+id/line10"
            >

            <TextView
                android:id="@+id/tvvariety"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="Variety"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_variety"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvvariety"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/line12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:layout_below="@+id/line11"
            >

            <TextView
                android:id="@+id/tvcanetype"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="Cane Type"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_canetype"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvcanetype"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/line13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:layout_below="@+id/line12"
            >

            <TextView
                android:id="@+id/tvcanearea"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="Plot Area"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_canearea"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvcanearea"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            >

            <TextView
                android:id="@+id/tvsharearea"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:text="Share Area"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/tv_sharearea"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="@color/black"
                android:layout_toRightOf="@+id/tvsharearea"
                android:text=""
                android:textSize="15dp" />

        </RelativeLayout>


    </LinearLayout>



</androidx.cardview.widget.CardView>