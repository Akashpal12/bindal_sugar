<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/form_bg"
    tools:context=".view.MainActivity">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            app:theme="@style/AppTheme.ToolbarFont"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorAccent"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

    </com.google.android.material.appbar.AppBarLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:context=".view.MainActivity"
        >

        <RelativeLayout
            android:layout_marginTop="10dp"
           android:background="@drawable/background_image_with_alpha"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.google.android.flexbox.FlexboxLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:alignContent="flex_start"
                        app:alignItems="flex_start"
                        app:flexWrap="wrap"
                        app:justifyContent="flex_start">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:baselineAligned="false"
                        android:id="@+id/line1"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight=".25"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="@dimen/dashboard_corner_radious"
                            app:cardElevation="@dimen/dashboard_card_elevation"
                            app:cardUseCompatPadding="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="4dp"
                                android:clickable="true"
                                android:onClick="openServerDataStatus"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/img_plot_finder"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:src="@drawable/targetreport" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_below="@+id/img_plot_finder"
                                    android:layout_marginTop="5dp"
                                    android:fontFamily="@font/roboto_black"
                                    android:gravity="center"
                                    android:text="@string/MENU_CHECK_PENDING_SERVER"
                                    android:textColor="@color/colorAccent"
                                    android:textSize="12dp"
                                    android:textStyle="bold" />

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight=".25"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="@dimen/dashboard_corner_radious"
                            app:cardElevation="@dimen/dashboard_card_elevation"
                            app:cardUseCompatPadding="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="4dp"
                                android:clickable="true"
                                android:onClick="openAllDataStatus"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/img_variety"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:src="@drawable/variertywisereport" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_below="@+id/img_variety"
                                    android:layout_marginTop="5dp"
                                    android:fontFamily="@font/roboto_black"
                                    android:gravity="center"
                                    android:text="@string/MENU_ALL_SURVEY_DATA"
                                    android:textColor="@color/colorAccent"
                                    android:textSize="12dp"
                                    android:textStyle="bold" />

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight=".25"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="@dimen/dashboard_corner_radious"
                            app:cardElevation="@dimen/dashboard_card_elevation"
                            app:cardUseCompatPadding="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="4dp"
                                android:clickable="true"
                                android:onClick="caneTypeSummery"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/img_supervisor_with_variety_area"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:src="@drawable/todayvarietywisereport" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_below="@+id/img_supervisor_with_variety_area"
                                    android:layout_marginTop="5dp"
                                    android:fontFamily="@font/roboto_black"
                                    android:gravity="center"
                                    android:text="@string/MENU_CANETYPE_SUMMARY"
                                    android:textColor="@color/colorAccent"
                                    android:textSize="12dp"
                                    android:textStyle="bold" />

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:baselineAligned="false"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight=".25"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="@dimen/dashboard_corner_radious"
                            app:cardElevation="@dimen/dashboard_card_elevation"
                            app:cardUseCompatPadding="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="4dp"
                                android:clickable="true"
                                android:onClick="plotWiseReport"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/img_survey_report"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:src="@drawable/survey" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_below="@+id/img_survey_report"
                                    android:layout_marginTop="5dp"
                                    android:fontFamily="@font/roboto_black"
                                    android:gravity="center"
                                    android:text="@string/MENU_GROWER_WISE_REPORT"
                                    android:textColor="@color/colorAccent"
                                    android:textSize="12dp"
                                    android:textStyle="bold" />

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight=".25"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="@dimen/dashboard_corner_radious"
                            app:cardElevation="@dimen/dashboard_card_elevation"
                            app:cardUseCompatPadding="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="4dp"
                                android:clickable="true"
                                android:onClick="checkListReport"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/img_seed_reservation_details"
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:src="@drawable/reservation" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_below="@+id/img_seed_reservation_details"
                                    android:layout_marginTop="5dp"
                                    android:fontFamily="@font/roboto_black"
                                    android:gravity="center"
                                    android:text="@string/MENU_CHECK_LIST_REPORT"
                                    android:textColor="@color/colorAccent"
                                    android:textSize="12dp"
                                    android:textStyle="bold" />

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight=".25"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="@dimen/dashboard_corner_radious"
                            app:cardElevation="@dimen/dashboard_card_elevation"
                            app:cardUseCompatPadding="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="4dp"
                                android:clickable="true"
                                android:onClick="openVarietyWiseSummary"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/img_seed_mapping_report"
