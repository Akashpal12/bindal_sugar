                            android:layout_width="0dp"
                            android:layout_height="120dp"
                            android:layout_gravity="left|top"
                            android:layout_weight="1"
                            android:clickable="true"
                            android:onClick="openCropObservation"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="12dp"
                            app:cardElevation="5dp"

                            app:cardUseCompatPadding="true">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="4dp"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:src="@drawable/crop" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:fontFamily="@font/roboto_black"
                                    android:gravity="center"
                                    android:text="@string/MENU_CROP_OBSERVATION_VIEW"
                                    android:textColor="@color/colorAccent"
                                    android:textSize="12dp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="@dimen/dashboard_corner_radious"
                            app:cardElevation="@dimen/dashboard_card_elevation"
                            app:cardUseCompatPadding="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="4dp"
                                android:clickable="true"
                                android:onClick="openCropObservationReport"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/img_seed_indent_3"
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:src="@drawable/ic_baseline_date_range_24" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_below="@+id/img_seed_indent_3"
                                    android:layout_marginTop="5dp"
                                    android:fontFamily="@font/roboto_black"
                                    android:gravity="center"
                                    android:text="@string/MENU_CANE_OBSERVATION_REPORT"
                                    android:textColor="@color/colorAccent"
                                    android:textSize="12dp"
                                    android:textStyle="bold" />

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight=".25"
                            android:visibility="gone"
                            app:cardBackgroundColor="@color/white"
                            app:cardCornerRadius="@dimen/dashboard_corner_radious"
                            app:cardElevation="@dimen/dashboard_card_elevation"
                            app:cardUseCompatPadding="true">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="4dp"
                                android:clickable="true"
                                android:onClick="openSurveyImportData"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/img_seed_indent4"
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:src="@drawable/ic_baseline_report_24" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_below="@+id/img_seed_indent4"
                                    android:layout_marginTop="5dp"
                                    android:fontFamily="@font/roboto_black"
                                    android:gravity="center"
                                    android:text="Type Of Planting Report"
                                    android:textColor="@color/colorAccent"
                                    android:textSize="12dp"
                                    android:textStyle="bold" />

                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                    </com.google.android.flexbox.FlexboxLayout>

